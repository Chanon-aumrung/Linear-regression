        "def selection_sort(L):\n",
        "    sortedList = []\n",
        "    for i in range(len(L)):\n",
        "        sortedList.append(min(L))\n",
        "        L.remove(min(L))\n",
        "        print(L)\n",
        "    return sortedList\n",
        "\n",

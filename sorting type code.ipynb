      "source": [
        "def selection_sort(L):\n",
        "  for i in range(0,len(L)):\n",
        "    min_index = L[i:].index(min(L[i:]))\n",
        "    L[i], L[min_index+i] = L[min_index+i], L[i]\n",
        "    print(L)\n",
        "\n",
        "L = [33, 1, 41, 5, 2, 53, 1]\n",
        "print(L)\n",
        "selection_sort(L)\n",
        "print(L)"
      ],

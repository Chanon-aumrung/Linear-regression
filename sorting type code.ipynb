        "def Bubble_sort(L):\n",
        "  for i in range(len(L)):\n",
        "    for j in range(len(L[:-i])):\n",
        "      if L[j] > L[j+1]:\n",
        "        L[j], L[j+1] = L[j+1], L[j]\n",
        "    print(L)\n",
        "  return L\n",
        "\n",
        "L = [30, 12, 5, 81, 4, 4,]\n",
        "\n",
        "print(L)\n",
        "print(Bubble_sort(L))"
